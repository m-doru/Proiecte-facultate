#include <bits/stdc++.h>

using namespace std;


int main()
{
    int t, c, d[105], v, dsize;
    int ans;
    freopen("default.in", "r", stdin);
//    freopen("default.out", "w", stdout);
    FILE *out = fopen("default.out", "w");
    scanf("%d", &t);
    for(int k = 0; k < t; ++k){
        ans = 0;

        scanf("%d %d %d", &c, &dsize, &v);

        for(int i = 0; i < dsize; ++i)
            scanf("%d", d+i);
        long long s = 0;
        for(int i = 0; i < dsize && s < v; ++i){
            if(s < d[i] - 1){
                while(s < d[i] - 1 && s < v){
                    s += (long long)c*(long long)(s+1);
                    ++ans;
                }
                s+=(long long)c*(long long)d[i];
            }
            else{
                s+=(long long)c*(long long)d[i];
            }
        }
        while(s < v){
            s += (long long)c*(long long)(s+1);
            ++ans;
        }
        fprintf(out,"Case #%d: %d\n", k+1, ans);
    }
    return 0;
}
